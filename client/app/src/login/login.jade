#login
  .container-fluid
    .row
      .col-md-4.col-md-push-4
      
        form
          input(
            ng-model="email"
            type="email"
            placeholder="email"
          )
          
          input(
            ng-model="password"
            type="password"
          )
          
          button(
            type="submit"
            ng-click="login()"
          ) Login
